{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft*": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft*": "Error"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft*": "Error"
      }
    },
    "File": {
      "LogLevel": "Critical",
      "MaxFileSizeInMB": 5
    }
  },
  "AllowedHosts": "*"
}
